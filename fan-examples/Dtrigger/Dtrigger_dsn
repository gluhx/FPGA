#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x561c3dd3bb20 .scope module, "Dtrigger_test" "Dtrigger_test" 2 2;
 .timescale -9 -11;
v0x561c3dd4d6d0_0 .var "D", 0 0;
v0x561c3dd4d790_0 .net "Q", 0 0, v0x561c3dd4d430_0;  1 drivers
v0x561c3dd4d860_0 .var "clk", 0 0;
v0x561c3dd4d960_0 .net "nQ", 0 0, v0x561c3dd4d590_0;  1 drivers
S_0x561c3dd3bcb0 .scope module, "T1" "Dtrigger" 2 21, 3 1 0, S_0x561c3dd3bb20;
 .timescale -9 -11;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "D";
    .port_info 2 /OUTPUT 1 "Q";
    .port_info 3 /OUTPUT 1 "nQ";
v0x561c3dd3bf00_0 .net "D", 0 0, v0x561c3dd4d6d0_0;  1 drivers
v0x561c3dd4d430_0 .var "Q", 0 0;
v0x561c3dd4d4f0_0 .net "clk", 0 0, v0x561c3dd4d860_0;  1 drivers
v0x561c3dd4d590_0 .var "nQ", 0 0;
E_0x561c3dd3c7d0 .event posedge, v0x561c3dd4d4f0_0;
    .scope S_0x561c3dd3bcb0;
T_0 ;
    %wait E_0x561c3dd3c7d0;
    %load/vec4 v0x561c3dd3bf00_0;
    %assign/vec4 v0x561c3dd4d430_0, 0;
    %load/vec4 v0x561c3dd3bf00_0;
    %nor/r;
    %assign/vec4 v0x561c3dd4d590_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x561c3dd3bb20;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c3dd4d860_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c3dd4d6d0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x561c3dd3bb20;
T_2 ;
    %vpi_call 2 5 "$dumpfile", "Dtrigger_sim.vcd" {0 0 0};
    %vpi_call 2 6 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x561c3dd3bb20 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x561c3dd3bb20;
T_3 ;
    %delay 500, 0;
    %load/vec4 v0x561c3dd4d860_0;
    %nor/r;
    %store/vec4 v0x561c3dd4d860_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x561c3dd3bb20;
T_4 ;
    %delay 700, 0;
    %load/vec4 v0x561c3dd4d6d0_0;
    %nor/r;
    %store/vec4 v0x561c3dd4d6d0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x561c3dd3bb20;
T_5 ;
    %vpi_call 2 25 "$monitor", "At time %t, out1=%b, out2=%b", $time, v0x561c3dd4d790_0, v0x561c3dd4d960_0 {0 0 0};
    %delay 50000, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Dtrigger_tb.v";
    "Dtrigger.v";
