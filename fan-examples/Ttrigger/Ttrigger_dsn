v0x55caa3694d30_0 .net "T", 0 0, v0x55caa3695070_0;  1 drivers
v0x55caa3694df0_0 .var "nQ", 0 0;
v0x55caa3694e90_0 .var "tmp_Q", 0 0;
E_0x55caa3683c10 .event posedge, v0x55caa3694d30_0;
    .scope S_0x55caa36834e0;
T_0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55caa3694e90_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x55caa36834e0;
T_1 ;
    %wait E_0x55caa3683c10;
    %load/vec4 v0x55caa3694e90_0;
    %nor/r;
    %store/vec4 v0x55caa3694e90_0, 0, 1;
    %load/vec4 v0x55caa3694e90_0;
    %assign/vec4 v0x55caa3683710_0, 0;
    %load/vec4 v0x55caa3694e90_0;
    %nor/r;
    %assign/vec4 v0x55caa3694df0_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55caa3683350;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55caa3695070_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x55caa3683350;
T_3 ;
    %vpi_call 2 5 "$dumpfile", "Ttrigger_sim.vcd" {0 0 0};
    %vpi_call 2 6 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55caa3683350 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55caa3683350;
T_4 ;
    %delay 500, 0;
    %load/vec4 v0x55caa3695070_0;
    %nor/r;
    %store/vec4 v0x55caa3695070_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x55caa3683350;
T_5 ;
    %vpi_call 2 21 "$monitor", "At time %t, out1=%b, out2=%b", $time, v0x55caa3694fd0_0, v0x55caa3695140_0 {0 0 0};
    %delay 50000, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_5;
