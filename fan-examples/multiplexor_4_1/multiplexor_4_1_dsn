#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x5557fb90cd10 .scope module, "multiplexor_4_1_test" "multiplexor_4_1_test" 2 2;
 .timescale -9 -11;
v0x5557fb91dde0_0 .var "D", 3 0;
v0x5557fb91dea0_0 .net "F", 0 0, L_0x5557fb91e070;  1 drivers
v0x5557fb91df70_0 .var "X", 1 0;
S_0x5557fb90cea0 .scope module, "MUX1" "multiplexor_4_1" 2 19, 3 1 0, S_0x5557fb90cd10;
 .timescale -9 -11;
    .port_info 0 /INPUT 4 "D";
    .port_info 1 /INPUT 2 "X";
    .port_info 2 /OUTPUT 1 "F";
v0x5557fb8fa680_0 .net "D", 3 0, v0x5557fb91dde0_0;  1 drivers
v0x5557fb91dbe0_0 .net "F", 0 0, L_0x5557fb91e070;  alias, 1 drivers
v0x5557fb91dca0_0 .net "X", 1 0, v0x5557fb91df70_0;  1 drivers
L_0x5557fb91e070 .part/v v0x5557fb91dde0_0, v0x5557fb91df70_0, 1;
    .scope S_0x5557fb90cd10;
T_0 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5557fb91df70_0, 0, 2;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x5557fb91dde0_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x5557fb90cd10;
T_1 ;
    %vpi_call 2 5 "$dumpfile", "multiplexor_4_1_sim.vcd" {0 0 0};
    %vpi_call 2 6 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5557fb90cd10 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5557fb90cd10;
T_2 ;
    %delay 500, 0;
    %load/vec4 v0x5557fb91df70_0;
    %addi 1, 0, 2;
    %store/vec4 v0x5557fb91df70_0, 0, 2;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5557fb90cd10;
T_3 ;
    %delay 1700, 0;
    %load/vec4 v0x5557fb91dde0_0;
    %pushi/vec4 15, 0, 4;
    %xor;
    %store/vec4 v0x5557fb91dde0_0, 0, 4;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5557fb90cd10;
T_4 ;
    %vpi_call 2 23 "$monitor", "At time %t, out1=%b", $time, v0x5557fb91dea0_0 {0 0 0};
    %delay 50000, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "multiplexor_4_1_tb.v";
    "multiplexor_4_1.v";
