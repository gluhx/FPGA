#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x559973ab2b20 .scope module, "syncTtrigger_test" "syncTtrigger_test" 2 2;
 .timescale -9 -11;
v0x559973ac5400_0 .net "Q", 0 0, v0x559973ab2f00_0;  1 drivers
v0x559973ac54c0_0 .var "T", 0 0;
v0x559973ac5560_0 .var "clk", 0 0;
v0x559973ac5660_0 .net "nQ", 0 0, v0x559973ac51b0_0;  1 drivers
S_0x559973ab2cb0 .scope module, "T1" "syncTtrigger" 2 21, 3 1 0, S_0x559973ab2b20;
 .timescale -9 -11;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "T";
    .port_info 2 /OUTPUT 1 "Q";
    .port_info 3 /OUTPUT 1 "nQ";
v0x559973ab2f00_0 .var "Q", 0 0;
v0x559973ac5050_0 .net "T", 0 0, v0x559973ac54c0_0;  1 drivers
v0x559973ac5110_0 .net "clk", 0 0, v0x559973ac5560_0;  1 drivers
v0x559973ac51b0_0 .var "nQ", 0 0;
v0x559973ac5270_0 .var "tmp_Q", 0 0;
E_0x559973ab3510 .event posedge, v0x559973ac5110_0;
    .scope S_0x559973ab2cb0;
T_0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559973ac5270_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x559973ab2cb0;
T_1 ;
    %wait E_0x559973ab3510;
    %load/vec4 v0x559973ac5050_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x559973ac5270_0;
    %nor/r;
    %store/vec4 v0x559973ac5270_0, 0, 1;
T_1.0 ;
    %load/vec4 v0x559973ac5270_0;
    %assign/vec4 v0x559973ab2f00_0, 0;
    %load/vec4 v0x559973ac5270_0;
    %nor/r;
    %assign/vec4 v0x559973ac51b0_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x559973ab2b20;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559973ac5560_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559973ac54c0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x559973ab2b20;
T_3 ;
    %vpi_call 2 5 "$dumpfile", "syncTtrigger_sim.vcd" {0 0 0};
    %vpi_call 2 6 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x559973ab2b20 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x559973ab2b20;
T_4 ;
    %delay 500, 0;
    %load/vec4 v0x559973ac5560_0;
    %nor/r;
    %store/vec4 v0x559973ac5560_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x559973ab2b20;
T_5 ;
    %delay 700, 0;
    %load/vec4 v0x559973ac54c0_0;
    %nor/r;
    %store/vec4 v0x559973ac54c0_0, 0, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x559973ab2b20;
T_6 ;
    %vpi_call 2 25 "$monitor", "At time %t, out1=%b, out2=%b", $time, v0x559973ac5400_0, v0x559973ac5660_0 {0 0 0};
    %delay 50000, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "syncTtrigger_tb.v";
    "syncTtrigger.v";
