#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x61c88b403f50 .scope module, "mult_test" "mult_test" 2 2;
 .timescale -11 -12;
v0x61c88b42ba80_0 .var "a", 7 0;
v0x61c88b42bb60_0 .var "b", 7 0;
v0x61c88b42bc30_0 .var "clk", 0 0;
v0x61c88b42bd30_0 .net "out", 15 0, v0x61c88b42b670_0;  1 drivers
S_0x61c88b418700 .scope module, "DD1" "mult" 2 23, 3 1 0, S_0x61c88b403f50;
 .timescale -11 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 8 "multipliable_1";
    .port_info 2 /INPUT 8 "multipliable_2";
    .port_info 3 /OUTPUT 16 "composition";
v0x61c88b404e80_0 .net "clk", 0 0, v0x61c88b42bc30_0;  1 drivers
v0x61c88b42b670_0 .var "composition", 15 0;
v0x61c88b42b750_0 .net "multipliable_1", 7 0, v0x61c88b42ba80_0;  1 drivers
v0x61c88b42b810_0 .net "multipliable_2", 7 0, v0x61c88b42bb60_0;  1 drivers
v0x61c88b42b8f0_0 .var "multipliables", 15 0;
E_0x61c88b419a50 .event posedge, v0x61c88b404e80_0;
    .scope S_0x61c88b418700;
T_0 ;
    %wait E_0x61c88b419a50;
    %load/vec4 v0x61c88b42b750_0;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x61c88b42b8f0_0, 4, 5;
    %load/vec4 v0x61c88b42b810_0;
    %ix/load 4, 8, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x61c88b42b8f0_0, 4, 5;
    %load/vec4 v0x61c88b42b8f0_0;
    %parti/s 8, 0, 2;
    %pad/u 16;
    %load/vec4 v0x61c88b42b8f0_0;
    %parti/s 8, 8, 5;
    %pad/u 16;
    %mul;
    %assign/vec4 v0x61c88b42b670_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x61c88b403f50;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61c88b42bc30_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x61c88b42ba80_0, 0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x61c88b42bb60_0, 0, 8;
    %end;
    .thread T_1;
    .scope S_0x61c88b403f50;
T_2 ;
    %vpi_call 2 5 "$dumpfile", "mult_sim.vcd" {0 0 0};
    %vpi_call 2 6 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x61c88b403f50 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x61c88b403f50;
T_3 ;
    %delay 50, 0;
    %load/vec4 v0x61c88b42bc30_0;
    %nor/r;
    %store/vec4 v0x61c88b42bc30_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x61c88b403f50;
T_4 ;
    %wait E_0x61c88b419a50;
    %load/vec4 v0x61c88b42ba80_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x61c88b42ba80_0, 0;
    %load/vec4 v0x61c88b42bb60_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x61c88b42bb60_0, 0;
    %jmp T_4;
    .thread T_4;
    .scope S_0x61c88b403f50;
T_5 ;
    %vpi_call 2 27 "$monitor", "At time %t", $time {0 0 0};
    %delay 5000, 0;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "code.v";
