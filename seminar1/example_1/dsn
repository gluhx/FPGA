#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x55c8bb1edb30 .scope module, "counter_testbench" "counter_testbench" 2 1;
 .timescale 0 0;
v0x55c8bb202470_0 .var "clk", 0 0;
v0x55c8bb202530_0 .var "in", 0 0;
v0x55c8bb202600_0 .net "out0", 0 0, v0x55c8bb201fc0_0;  1 drivers
v0x55c8bb202700_0 .net "out1", 0 0, v0x55c8bb202080_0;  1 drivers
v0x55c8bb2027d0_0 .net "out2", 0 0, v0x55c8bb202120_0;  1 drivers
S_0x55c8bb1edcc0 .scope module, "c1" "counter" 2 20, 3 1 0, S_0x55c8bb1edb30;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "x_in";
    .port_info 2 /OUTPUT 1 "out0";
    .port_info 3 /OUTPUT 1 "out1";
    .port_info 4 /OUTPUT 1 "out2";
v0x55c8bb1edea0_0 .net "clk", 0 0, v0x55c8bb202470_0;  1 drivers
v0x55c8bb201fc0_0 .var "out0", 0 0;
v0x55c8bb202080_0 .var "out1", 0 0;
v0x55c8bb202120_0 .var "out2", 0 0;
v0x55c8bb2021e0_0 .var "state", 1 0;
v0x55c8bb202310_0 .net "x_in", 0 0, v0x55c8bb202530_0;  1 drivers
E_0x55c8bb1ee780 .event posedge, v0x55c8bb1edea0_0;
    .scope S_0x55c8bb1edcc0;
T_0 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55c8bb2021e0_0, 0, 2;
    %end;
    .thread T_0;
    .scope S_0x55c8bb1edcc0;
T_1 ;
    %wait E_0x55c8bb1ee780;
    %load/vec4 v0x55c8bb202310_0;
    %load/vec4 v0x55c8bb2021e0_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %assign/vec4 v0x55c8bb201fc0_0, 0;
    %load/vec4 v0x55c8bb202310_0;
    %load/vec4 v0x55c8bb2021e0_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %assign/vec4 v0x55c8bb202080_0, 0;
    %load/vec4 v0x55c8bb202310_0;
    %load/vec4 v0x55c8bb2021e0_0;
    %pad/u 32;
    %pushi/vec4 2, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %assign/vec4 v0x55c8bb202120_0, 0;
    %load/vec4 v0x55c8bb202310_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x55c8bb2021e0_0;
    %pad/u 32;
    %cmpi/e 2, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55c8bb2021e0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x55c8bb2021e0_0;
    %addi 1, 0, 2;
    %store/vec4 v0x55c8bb2021e0_0, 0, 2;
T_1.3 ;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55c8bb1edb30;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c8bb202470_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c8bb202530_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x55c8bb1edb30;
T_3 ;
    %vpi_call 2 3 "$dumpfile", "test.vcd" {0 0 0};
    %vpi_call 2 4 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55c8bb1edb30 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55c8bb1edb30;
T_4 ;
    %delay 5, 0;
    %load/vec4 v0x55c8bb202470_0;
    %nor/r;
    %store/vec4 v0x55c8bb202470_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x55c8bb1edb30;
T_5 ;
    %delay 5, 0;
    %load/vec4 v0x55c8bb202530_0;
    %nor/r;
    %store/vec4 v0x55c8bb202530_0, 0, 1;
T_5.0 ;
    %delay 7, 0;
    %load/vec4 v0x55c8bb202530_0;
    %nor/r;
    %store/vec4 v0x55c8bb202530_0, 0, 1;
    %delay 12, 0;
    %load/vec4 v0x55c8bb202530_0;
    %nor/r;
    %store/vec4 v0x55c8bb202530_0, 0, 1;
    %jmp T_5.0;
    %end;
    .thread T_5;
    .scope S_0x55c8bb1edb30;
T_6 ;
    %vpi_call 2 22 "$monitor", "At time %t, out0 = %b, out1 = %b, out2 = %b, state = %d", $time, v0x55c8bb202600_0, v0x55c8bb202700_0, v0x55c8bb2027d0_0, v0x55c8bb2021e0_0 {0 0 0};
    %delay 300, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
