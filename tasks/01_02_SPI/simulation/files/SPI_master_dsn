#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x57f2d5025280 .scope module, "SPI_master_test" "SPI_master_test" 2 4;
 .timescale -11 -12;
v0x57f2d5046270_0 .var "clk", 0 0;
v0x57f2d5046330_0 .net "cs", 0 0, v0x57f2d5045a60_0;  1 drivers
v0x57f2d50463d0_0 .var "data", 7 0;
v0x57f2d50464a0_0 .var "en", 0 0;
v0x57f2d5046570_0 .net "mosi", 0 0, v0x57f2d5045f50_0;  1 drivers
v0x57f2d5046610_0 .var "reset", 0 0;
v0x57f2d50466e0_0 .net "sck", 0 0, v0x57f2d50460d0_0;  1 drivers
S_0x57f2d5025410 .scope module, "SPI_MASTER" "SPI_master" 2 33, 3 1 0, S_0x57f2d5025280;
 .timescale -11 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "en_transit";
    .port_info 3 /INPUT 8 "data";
    .port_info 4 /OUTPUT 1 "sck";
    .port_info 5 /OUTPUT 1 "mosi";
    .port_info 6 /OUTPUT 1 "cs";
v0x57f2d50294e0_0 .net "clk", 0 0, v0x57f2d5046270_0;  1 drivers
v0x57f2d5045980_0 .var "counter_bit", 2 0;
v0x57f2d5045a60_0 .var "cs", 0 0;
v0x57f2d5045b00_0 .net "data", 7 0, v0x57f2d50463d0_0;  1 drivers
v0x57f2d5045be0_0 .net "en_transit", 0 0, v0x57f2d50464a0_0;  1 drivers
v0x57f2d5045cf0_0 .var "flag_sck", 0 0;
v0x57f2d5045db0_0 .var "flag_transit", 0 0;
v0x57f2d5045e70_0 .var "memory", 7 0;
v0x57f2d5045f50_0 .var "mosi", 0 0;
v0x57f2d5046010_0 .net "reset", 0 0, v0x57f2d5046610_0;  1 drivers
v0x57f2d50460d0_0 .var "sck", 0 0;
E_0x57f2d502a810/0 .event negedge, v0x57f2d5046010_0;
E_0x57f2d502a810/1 .event posedge, v0x57f2d50294e0_0;
E_0x57f2d502a810 .event/or E_0x57f2d502a810/0, E_0x57f2d502a810/1;
    .scope S_0x57f2d5025410;
T_0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57f2d50460d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57f2d5045f50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57f2d5045a60_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x57f2d5045e70_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57f2d5045db0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57f2d5045cf0_0, 0, 1;
    %pushi/vec4 7, 0, 3;
    %store/vec4 v0x57f2d5045980_0, 0, 3;
    %end;
    .thread T_0;
    .scope S_0x57f2d5025410;
T_1 ;
    %wait E_0x57f2d502a810;
    %load/vec4 v0x57f2d5046010_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x57f2d5045e70_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x57f2d5045db0_0, 0;
    %pushi/vec4 7, 0, 3;
    %assign/vec4 v0x57f2d5045980_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x57f2d50460d0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x57f2d5045f50_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x57f2d5045a60_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x57f2d5045be0_0;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_1.4, 9;
    %load/vec4 v0x57f2d5045db0_0;
    %nor/r;
    %and;
T_1.4;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x57f2d5045db0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x57f2d5045a60_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x57f2d50460d0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x57f2d5045f50_0, 0;
    %load/vec4 v0x57f2d5045b00_0;
    %assign/vec4 v0x57f2d5045e70_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x57f2d5045cf0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x57f2d5045db0_0;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_1.7, 9;
    %load/vec4 v0x57f2d5045cf0_0;
    %nor/r;
    %and;
T_1.7;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.5, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x57f2d5045db0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x57f2d50460d0_0, 0;
T_1.5 ;
    %load/vec4 v0x57f2d5045cf0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.8, 8;
    %load/vec4 v0x57f2d50460d0_0;
    %inv;
    %assign/vec4 v0x57f2d50460d0_0, 0;
    %load/vec4 v0x57f2d50460d0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.10, 8;
    %load/vec4 v0x57f2d5045e70_0;
    %load/vec4 v0x57f2d5045980_0;
    %part/u 1;
    %assign/vec4 v0x57f2d5045f50_0, 0;
    %load/vec4 v0x57f2d5045980_0;
    %cmpi/e 0, 0, 3;
    %jmp/0xz  T_1.12, 4;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x57f2d5045cf0_0, 0;
    %pushi/vec4 7, 0, 3;
    %assign/vec4 v0x57f2d5045980_0, 0;
    %jmp T_1.13;
T_1.12 ;
    %load/vec4 v0x57f2d5045980_0;
    %subi 1, 0, 3;
    %assign/vec4 v0x57f2d5045980_0, 0;
T_1.13 ;
T_1.10 ;
T_1.8 ;
    %load/vec4 v0x57f2d5045db0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.14, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x57f2d50460d0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x57f2d5045f50_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x57f2d5045a60_0, 0;
T_1.14 ;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x57f2d5025280;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57f2d5046270_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57f2d5046610_0, 0, 1;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x57f2d50463d0_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57f2d50464a0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x57f2d5025280;
T_3 ;
    %vpi_call 2 8 "$dumpfile", "SPI_master_simulation.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x57f2d5025280 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x57f2d5025280;
T_4 ;
    %delay 50, 0;
    %load/vec4 v0x57f2d5046270_0;
    %inv;
    %assign/vec4 v0x57f2d5046270_0, 0;
    %jmp T_4;
    .thread T_4;
    .scope S_0x57f2d5025280;
T_5 ;
    %delay 190, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57f2d50464a0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57f2d50464a0_0, 0, 1;
    %delay 3100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57f2d50464a0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57f2d50464a0_0, 0, 1;
    %end;
    .thread T_5;
    .scope S_0x57f2d5025280;
T_6 ;
    %vpi_call 2 45 "$monitor", "At time %t, en - %b, cs - %b, sck - %b, mosi - %b", $time, v0x57f2d50464a0_0, v0x57f2d5046330_0, v0x57f2d50466e0_0, v0x57f2d5046570_0 {0 0 0};
    %delay 4000, 0;
    %vpi_call 2 46 "$finish" {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbenches/SPI_master_tb.v";
    "code/SPI_master.v";
