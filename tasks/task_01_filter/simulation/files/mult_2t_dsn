#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x55f9932c71d0 .scope module, "mult_2t_test" "mult_2t_test" 2 4;
 .timescale -11 -12;
v0x55f9932e1ab0_0 .var "clk", 0 0;
v0x55f9932e1b70_0 .var "enable", 0 0;
v0x55f9932e1c40_0 .net "out", 31 0, v0x55f9932e1680_0;  1 drivers
v0x55f9932e1d40_0 .var "reset", 0 0;
v0x55f9932e1e10_0 .var "x", 31 0;
E_0x55f9932ccf00 .event posedge, v0x55f9932cbb60_0;
S_0x55f9932c7360 .scope module, "MULT" "mult_2t" 2 35, 3 2 0, S_0x55f9932c71d0;
 .timescale -11 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "enable";
    .port_info 3 /INPUT 32 "multipliable_1";
    .port_info 4 /INPUT 32 "multipliable_2";
    .port_info 5 /OUTPUT 32 "mult_result";
v0x55f9932cbb60_0 .net "clk", 0 0, v0x55f9932e1ab0_0;  1 drivers
v0x55f9932e1500_0 .net "enable", 0 0, v0x55f9932e1b70_0;  1 drivers
v0x55f9932e15c0_0 .var "mult_delay", 31 0;
v0x55f9932e1680_0 .var "mult_result", 31 0;
v0x55f9932e1760_0 .net "multipliable_1", 31 0, v0x55f9932e1e10_0;  1 drivers
v0x55f9932e1890_0 .net "multipliable_2", 31 0, v0x55f9932e1e10_0;  alias, 1 drivers
v0x55f9932e1950_0 .net "reset", 0 0, v0x55f9932e1d40_0;  1 drivers
E_0x55f9932cd690/0 .event negedge, v0x55f9932e1950_0;
E_0x55f9932cd690/1 .event posedge, v0x55f9932cbb60_0;
E_0x55f9932cd690 .event/or E_0x55f9932cd690/0, E_0x55f9932cd690/1;
    .scope S_0x55f9932c7360;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55f9932e1680_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55f9932e15c0_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x55f9932c7360;
T_1 ;
    %wait E_0x55f9932cd690;
    %load/vec4 v0x55f9932e1950_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x55f9932e15c0_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x55f9932e1680_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55f9932e1500_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x55f9932e1760_0;
    %load/vec4 v0x55f9932e1890_0;
    %mul;
    %assign/vec4 v0x55f9932e15c0_0, 0;
T_1.2 ;
    %load/vec4 v0x55f9932e15c0_0;
    %assign/vec4 v0x55f9932e1680_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55f9932c71d0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f9932e1ab0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55f9932e1e10_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f9932e1d40_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f9932e1b70_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x55f9932c71d0;
T_3 ;
    %vpi_call 2 7 "$dumpfile", "mult_2t_simulation.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55f9932c71d0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55f9932c71d0;
T_4 ;
    %delay 50, 0;
    %load/vec4 v0x55f9932e1ab0_0;
    %nor/r;
    %store/vec4 v0x55f9932e1ab0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x55f9932c71d0;
T_5 ;
    %wait E_0x55f9932ccf00;
    %load/vec4 v0x55f9932e1e10_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x55f9932e1e10_0, 0;
    %jmp T_5;
    .thread T_5;
    .scope S_0x55f9932c71d0;
T_6 ;
    %delay 910, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55f9932e1d40_0, 0;
    %end;
    .thread T_6;
    .scope S_0x55f9932c71d0;
T_7 ;
    %delay 500, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55f9932e1b70_0, 0;
    %delay 200, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55f9932e1b70_0, 0;
    %end;
    .thread T_7;
    .scope S_0x55f9932c71d0;
T_8 ;
    %vpi_call 2 46 "$monitor", "At time %t, reset : %b, en : %b, in : %b, out %b", $time, v0x55f9932e1d40_0, v0x55f9932e1b70_0, v0x55f9932e1e10_0, v0x55f9932e1c40_0 {0 0 0};
    %delay 2000, 0;
    %vpi_call 2 47 "$finish" {0 0 0};
    %end;
    .thread T_8;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbenches/mult_2t_tb.v";
    "code/mult_2t.v";
