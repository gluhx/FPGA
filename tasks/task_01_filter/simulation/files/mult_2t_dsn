#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x62416cf151d0 .scope module, "mult_2t_test" "mult_2t_test" 2 4;
 .timescale -11 -12;
v0x62416cf2f110_0 .var "clk", 0 0;
v0x62416cf2f1d0_0 .var "enable", 0 0;
v0x62416cf2f2a0_0 .net "out", 7 0, v0x62416cf2ece0_0;  1 drivers
v0x62416cf2f3a0_0 .var "reset", 0 0;
v0x62416cf2f470_0 .var "x", 7 0;
E_0x62416cf1ae90 .event posedge, v0x62416cf19af0_0;
S_0x62416cf15360 .scope module, "MULT" "mult_2t" 2 35, 3 2 0, S_0x62416cf151d0;
 .timescale -11 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "enable";
    .port_info 3 /INPUT 8 "multipliable_1";
    .port_info 4 /INPUT 8 "multipliable_2";
    .port_info 5 /OUTPUT 8 "mult_result";
v0x62416cf19af0_0 .net "clk", 0 0, v0x62416cf2f110_0;  1 drivers
v0x62416cf2eb60_0 .net "enable", 0 0, v0x62416cf2f1d0_0;  1 drivers
v0x62416cf2ec20_0 .var "mult_delay", 31 0;
v0x62416cf2ece0_0 .var "mult_result", 7 0;
v0x62416cf2edc0_0 .net "multipliable_1", 7 0, v0x62416cf2f470_0;  1 drivers
v0x62416cf2eef0_0 .net "multipliable_2", 7 0, v0x62416cf2f470_0;  alias, 1 drivers
v0x62416cf2efb0_0 .net "reset", 0 0, v0x62416cf2f3a0_0;  1 drivers
E_0x62416cf1b3c0/0 .event negedge, v0x62416cf2efb0_0;
E_0x62416cf1b3c0/1 .event posedge, v0x62416cf19af0_0;
E_0x62416cf1b3c0 .event/or E_0x62416cf1b3c0/0, E_0x62416cf1b3c0/1;
    .scope S_0x62416cf15360;
T_0 ;
    %wait E_0x62416cf1b3c0;
    %load/vec4 v0x62416cf2efb0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x62416cf2ec20_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x62416cf2ece0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x62416cf2eb60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x62416cf2edc0_0;
    %pad/u 32;
    %load/vec4 v0x62416cf2eef0_0;
    %pad/u 32;
    %mul;
    %assign/vec4 v0x62416cf2ec20_0, 0;
T_0.2 ;
    %load/vec4 v0x62416cf2ec20_0;
    %pad/u 8;
    %assign/vec4 v0x62416cf2ece0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x62416cf151d0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62416cf2f110_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x62416cf2f470_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62416cf2f3a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62416cf2f1d0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x62416cf151d0;
T_2 ;
    %vpi_call 2 7 "$dumpfile", "mult_2t_simulation.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x62416cf151d0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x62416cf151d0;
T_3 ;
    %delay 50, 0;
    %load/vec4 v0x62416cf2f110_0;
    %nor/r;
    %store/vec4 v0x62416cf2f110_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x62416cf151d0;
T_4 ;
    %wait E_0x62416cf1ae90;
    %load/vec4 v0x62416cf2f470_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x62416cf2f470_0, 0;
    %jmp T_4;
    .thread T_4;
    .scope S_0x62416cf151d0;
T_5 ;
    %delay 910, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x62416cf2f3a0_0, 0;
    %end;
    .thread T_5;
    .scope S_0x62416cf151d0;
T_6 ;
    %delay 500, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x62416cf2f1d0_0, 0;
    %delay 200, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x62416cf2f1d0_0, 0;
    %end;
    .thread T_6;
    .scope S_0x62416cf151d0;
T_7 ;
    %vpi_call 2 46 "$monitor", "At time %t, reset : %b, en : %b, in : %b, out %b", $time, v0x62416cf2f3a0_0, v0x62416cf2f1d0_0, v0x62416cf2f470_0, v0x62416cf2f2a0_0 {0 0 0};
    %delay 2000, 0;
    %vpi_call 2 47 "$finish" {0 0 0};
    %end;
    .thread T_7;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbenches/mult_2t_tb.v";
    "src/mult_2t.v";
