    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5e8378bad930_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5e8378bad870_0, 0;
