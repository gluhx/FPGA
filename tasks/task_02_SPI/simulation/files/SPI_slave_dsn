S_0x5e8378b874a0 .scope module, "SPI_slave_test" "SPI_slave_test" 2 7;
 .timescale -11 -12;
v0x5e8378bae990_0 .net "addr", 15 0, v0x5e8378badfc0_0;  1 drivers
