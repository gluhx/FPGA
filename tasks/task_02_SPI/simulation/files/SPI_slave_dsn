v0x5e8378badb90_0 .var "sck", 0 0;
E_0x5e8378b80550/0 .event negedge, v0x5e8378badad0_0;
E_0x5e8378b80550/1 .event posedge, v0x5e8378b8da30_0;
E_0x5e8378b80550 .event/or E_0x5e8378b80550/0, E_0x5e8378b80550/1;
S_0x5e8378badd30 .scope module, "SPI_Slave" "SPI_slave" 2 56, 4 1 0, S_0x5e8378b874a0;
 .timescale -11 -12;
    .port_info 0 /INPUT 1 "cs";
    .port_info 1 /INPUT 1 "sck";
    .port_info 2 /INPUT 1 "mosi";
    .port_info 3 /OUTPUT 8 "cmd";
    .port_info 4 /OUTPUT 16 "addr";
    .port_info 5 /OUTPUT 32 "data";
v0x5e8378badfc0_0 .var "addr", 15 0;
v0x5e8378bae0a0_0 .var "cmd", 7 0;
v0x5e8378bae180_0 .var "counter_bit", 2 0;
