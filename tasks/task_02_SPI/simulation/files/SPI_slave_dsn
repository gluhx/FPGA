    %jmp/0xz  T_1.8, 8;
    %load/vec4 v0x5e8378badb90_0;
    %inv;
    %assign/vec4 v0x5e8378badb90_0, 0;
    %load/vec4 v0x5e8378badb90_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.10, 8;
    %load/vec4 v0x5e8378bad930_0;
