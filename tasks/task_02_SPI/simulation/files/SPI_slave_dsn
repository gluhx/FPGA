    %pushi/vec4 7, 0, 3;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_3.4;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.2, 8;
    %load/vec4 v0x5e8378bae810_0;
    %ix/load 5, 0, 0;
    %load/vec4 v0x5e8378bae240_0;
    %pad/u 6;
    %muli 8, 0, 6;
    %ix/vec4 4;
    %assign/vec4/off/d v0x5e8378bae4e0_0, 4, 5;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5e8378bae240_0, 0;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5e8378bae180_0, 0;
