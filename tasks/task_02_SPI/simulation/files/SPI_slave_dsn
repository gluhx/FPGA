    %load/vec4 v0x5e8378bae5d0_0;
    %ix/getv 4, v0x5e8378bae180_0;
    %store/vec4 v0x5e8378bae810_0, 4, 1;
    %load/vec4 v0x5e8378bae240_0;
    %cmpi/e 6, 0, 3;
